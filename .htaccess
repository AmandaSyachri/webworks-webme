<IfModule mod_deflate.c>
	SetOutputFilter DEFLATE
</IfModule>

php_flag magic_quotes_gpc off

RewriteEngine on

RewriteCond %{SCRIPT_NAME} !\.php
RewriteRule ^admin[/]*$ /ww.admin [L]
RewriteRule ^favicon.ico$ /i/favicon.ico
RewriteRule ^product_subtypes/(.*) /p/product_subtypes.php?id=$1 [L]
RewriteRule ^\.productsearch(.*)$ /index.php?wwSpecial=productsearch$1 [L]
RewriteRule ^css$ /ww.css/all.php [L]
RewriteRule ^js_lang/([^/]*)/(.*)$ /j/lang.php [L]
RewriteRule ^js/(.*)$ /j/js.php [L]
RewriteRule ^kfmget/(.*)$ /j/fckeditor-2.6.3/editor/plugins/kfm/get.php?id=$1 [L]
RewriteRule ^([^./]*).rss$ /p/rss.php?page=$1 [L]
RewriteRule ^sitemap.xml /p/sitemap.php [L]
RewriteRule ^([^./]{3}[^.&]*)\&(.*)$ /index.php?page=$1&$2 [L]
RewriteRule ^([^./]{3}[^.]*)$ /index.php?page=$1 [QSA,L]

ErrorDocument 404 /p/404.php

<Files archives>
	ForceType application/x-httpd-php
</Files>

BrowserMatch "MSIE" brokenvary=1
BrowserMatch "Mozilla/4.[0-9]{2}" brokenvary=1
BrowserMatch "Opera" !brokenvary
SetEnvIf brokenvary 1 force-no-vary
ExpiresActive On
ExpiresDefault A18000
ExpiresByType image/gif A2592000
ExpiresByType image/jpeg A2592000
ExpiresByType image/png A2592000

AddCharset utf-8 .js
AddCharset utf-8 .php
